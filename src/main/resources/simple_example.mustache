insert overwrite table odpp_acc.marketdata_t partition(bdate='${bdate}', branch='${branch}')
select 
row_number() over(order by t.baseccy) as marketdataid
,t.bdate                    as marketdate
,concat('Exch.', t.baseccy) as ccypair
,t.fixing                   as exchangerate
from odpp.exchangerate t
where 1 = 1
  and t.bdate = '${bdate}'
  and t.baseCCY <> 'RUR'
  and t.counterCCY = 'RUB'
;
